llax>\n            <input type=\"color\" title=\"Color\" value=\"#d9c062\" selected>\n        </fieldset>\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"footer-head\">\n        <div class=\"img-bg\" mbr-if=\"showImgBg\"></div>\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"content-container col-12\">\n                    <div class=\"col-12 col-lg-6 col-brand\">\n                        <div class=\"navbar-brand\" mbr-if=\"showLogo || showBrand\">\n                        <span mbr-if=\"showLogo\" class=\"navbar-logo\">\n                        <a href=\"https://mobiri.se\">\n                            <img src=\"@PROJECT_PATH@/assets/images/chauffgae-granule-100x100.png\" alt=\"Mobirise\" mbr-style=\"{'height': logoSize + 'rem'}\">\n                        </a>\n                        </span>\n                            <span mbr-if=\"showBrand\" mbr-buttons mbr-theme-style=\"display-7\" class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove,-iconFont\"><a class=\"navbar-caption text-black\" data-app-selector=\".navbar-caption\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">CHAUFFAGE GRANULES</a></span>\n                        </div>\n                        <p class=\"brand-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".brand-text\" mbr-if=\"showText\">GREEN ENERGY Group\n<br>253 Rue Saint Honor√©\n<br>75001 Paris</p>\n                    </div>\n                    <div class=\"col-links col-12 col-lg-6\">\n                        <div class=\"social-row col-12 display-7\" mbr-if=\"showSocial\">\n                            <div class=\"soc-item\">\n                                <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon socicon-facebook\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                                <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon socicon-twitter\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                                <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                                <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                                </a>\n                            </div>\n                        </div>\n\n                        <div class=\"col-12 col-lg-6 col-xl-4 col-link1 col-link\">\n                            <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">Email: contact@greenenergygroup.fr</h5>\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".list\"><br></li>\n                            </ul>\n                        </div>\n                        <div class=\"col-12 col-lg-6 col-xl-4 col-link2 col-link\">\n                            <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\"><div>TVA Intra Comminautaire<br>FRQ0 58422180067</div></h5>\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">RCS Paris: 422180067 00056<br><br></li>\n                            </ul>\n                        </div>\n                        <div class=\"col-12 col-lg-6 col-xl-4 col-link3 col-link\">\n                            <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">Mentions L√©gales<br><br></h5>\n                            <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list\" data-multiline=\"true\" data-app-edit=\"full\" mbr-article>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".list\">CGV<br><br></li>\n                                <li class=\"mbr-text item-wrap\" data-app-selector=\".list\"><br></li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"footer-copyright\" mbr-if=\"showCopyright\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-12 col-copyright\">\n                    <p class=\"mbr-text mb-0 mbr-fonts-style copyright align-left\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">\n                        ¬© 2022 chauffage-granules.com - GREEN ENERGY Group Mandataire de<a href=\"https://jsecologie.fr/\" class=\"text-white\" target=\"_blank\"> js ecologie</a>- All Rights Reserved\n                    </p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
